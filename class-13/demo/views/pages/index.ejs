<!DOCTYPE html>
<html lang="en">
  <%- include('../partials/head') %>
<body>
  <%- include('../partials/header') %>

  <main>
    <ul>
      <% arrayOfTasks.forEach(task => { %>
        <li><%= task.title %></li>
        <li><%= task.description %></li>
        <li><%= task.contact %></li>
        <li><%= task.status %></li>
        <a href="/tasks/<%= task.id %>">Detail View</a>
      <% }) %>
    </ul>
  </main>
</body>
</html>